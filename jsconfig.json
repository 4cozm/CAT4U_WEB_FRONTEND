{
  "compilerOptions": {
    "jsx": "react-jsx",
    "checkJs": false,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": { "@/*": ["*"] },
    "types": ["node", "react", "react-dom", "next"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
